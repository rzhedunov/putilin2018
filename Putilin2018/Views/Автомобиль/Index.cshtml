@model IEnumerable<Putilin2018.Models.Автомобиль>

@{
    ViewBag.Title = "Список";
}

<h2>Список</h2>

<p>
    @Html.ActionLink("Добавить", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.Название_автомобиля)*@
            <label class="control-label col-md-2">Название автомобиля</label>
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Тип_ТС.Название_типа)*@
            <label class="control-label col-md-2">Тип</label>
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Госномер)*@
            <label class="control-label col-md-2">Госномер</label>
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Примечание)*@
            <label class="control-label col-md-2">Примечание</label>
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Название_автомобиля)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Тип_ТС.Название_типа)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Госномер)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Примечание)
        </td>
        <td>
            @Html.ActionLink("Изменить", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Данные", "Details", new { id=item.Id }) |
            @Html.ActionLink("Удалить", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
